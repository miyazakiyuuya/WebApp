{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "ES5",
    "lib": [ "ES2015", "DOM" ],
    "esModuleInterop": true,
    "jsx": "react",
    "outDir": ".tsoutput",
    "moduleResolution": "Node"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}